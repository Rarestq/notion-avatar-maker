{
    "version": 2,
    "builds": [
      {
        "src": "src/pages/api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/img/(.*)",
        "dest": "/src/pages/api/img/$1"
      },
      {
        "src": "/api/svg/(.*)",
        "dest": "/src/pages/api/svg/$1"
      }
    ],
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
    }
  }
  